<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceContainer name="content">
        <block class="Formax\ContactForm\Block\Solicitud" name="form_solicitid" template="Formax_ContactForm::solicitud.phtml" />
    </referenceContainer>
    
    <referenceBlock name="page.main.title" remove="true" />
    <referenceBlock name="header.panel" remove="true"/>
    <referenceBlock name="navigation.sections" remove="true"/>
    <referenceContainer name="xpec-header-search" remove="true"/>
    <referenceContainer name="footer" remove="true"/>
    <referenceBlock name="footer-block-afterall" remove="true"/>
    <referenceContainer name="xpec-header-logo" htmlClass="xpec-header-logo simulator-logo"/>
</page>